<AnchorPane prefHeight="540.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project1_0.Controller">
   <children>
      <Button fx:id="gbButton" alignment="CENTER" layoutX="25.0" layoutY="490.0" mnemonicParsing="false" onAction="#backToSelecting" text="Back">
