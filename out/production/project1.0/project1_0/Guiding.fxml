            <Font size="18.0" />
         </font>
      </Label>
      <ProgressBar fx:id="progressBar" layoutX="20.0" layoutY="37.0" prefHeight="32.0" prefWidth="110.0" progress="0.25" />
      <Button fx:id="nextButton" alignment="CENTER" layoutX="482.0" layoutY="35.0" mnemonicParsing="false" onAction="#nextAndIncreaseProgress" prefHeight="24.0" prefWidth="87.0" text="Next">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
